{
    "author": "xulinagliang",
    "description": "select * from learn",
    "gitbook": "3.2.3",
    "language": "zh-hans",
    "links": {
        "sidebar": {}
    },
    "output": {
        "pdf": true
    },
    "pdf": {
        "fontFamily": "Arial",
        "fontSize": 12,
        "margin": {
            "bottom": 56,
            "left": 62,
            "right": 62,
            "top": 56
        },
        "pageNumbers": true,
        "paperSize": "a4"
    },
    "plugins": [
        "-lunr", 
        "-search",
        "search-pro",
        "-fontsettings",
        "code",
        "tbfed-pagefooter",
        "hide-element",
        "back-to-top-button",
        "page-toc-button",
        "expandable-chapters-small",
        "sidebar-style",
        "splitter",
        "-sharing"
    ],
    "pluginsConfig": {
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy 徐亮亮 2024",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "hide-element": {
            "elements": [
                ".gitbook-link"
            ]
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        },
        "sidebar-style": {
            "title": "《前端开发文档》",
            "author": "徐亮亮"
        }
    },
    "structure": {
        "readme": "README.md"
    },
    "styles": {
        "website": "src/styles/website.css"
    },
    "title": "前端开发文档"
}